{
  "name": "puppet_kafka",
  "version": "0.1.0",
  "author": "Daniel van der Ende",
  "summary": "This module installs an Apache Kafka broker using either a custom rpm, or confluent's provided rpm",
  "license": "Apache 2.0",
  "source": "https://github.com/danielvdende/puppet-kafka",
  "project_page": "https://github.com/danielvdende/puppet-kafka",
  "issues_url": "https://github.com/danielvdende/puppet-kafka/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.5.1"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": ">= 2.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}
